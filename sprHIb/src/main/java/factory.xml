<?xml version="1.0" encoding="UTF-8"?>
<p:beans xmlns:p="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans spring-beans-3.2.xsd ">

	<p:bean id="pointFactory"
		class="spring.ioc.PointInstanceFactory">
		<p:constructor-arg value="100"></p:constructor-arg>
		<p:constructor-arg value="200"></p:constructor-arg>
	</p:bean>

	<p:bean id="p2" factory-bean="pointFactory"
		factory-method="createPoint">
		<p:constructor-arg value="120"></p:constructor-arg>
		<p:constructor-arg value="240"></p:constructor-arg>
	</p:bean>


	<p:bean id="p1" class="spring.ioc.PointFactory"
		factory-method="createPoint" scope="prototype">
		<!-- Scope values - singleton*, prototype, request, session -->
		<p:constructor-arg value="10"></p:constructor-arg>
		<p:constructor-arg value="20"></p:constructor-arg>

	</p:bean>

</p:beans>
