<?xml version="1.0" encoding="UTF-8"?>
<p:beans xmlns:p="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans spring-beans-3.2.xsd ">

	
	<p:import resource="classpath:ioc.xml"/>
	
	<p:bean id="siAbs" abstract="true">
		<p:property name="arithmetic">
			<p:bean class="spring.ioc.ArithmeticImpl"></p:bean>
		</p:property>
	</p:bean>
	
	<p:bean id="si" class="spring.ioc.SimpleInterestImpl" parent="siAbs">
	</p:bean>

	<p:bean name="utils" class="spring.ioc.UtilsBean">
		<p:property name="props">
			<p:props>
				<p:prop key="name">String</p:prop>
				<p:prop key="age">int</p:prop>
				<p:prop key="address">String</p:prop>
				<p:prop key="salary">float</p:prop>
			</p:props>
		</p:property>
	
		<p:property name="names">
			<p:set>
				<p:value>Abc</p:value>
				<p:value>Pqr</p:value>
				<p:value>Lmn</p:value>
				<p:value>Pqr</p:value>										<!-- set cannot have duplicates -->
			</p:set>
		</p:property>
		<p:property name="points">
			<p:list>
				<p:ref bean="start"/>
				<p:ref bean="end"/>
				<p:ref bean="start"/>									<!-- list can have duplicates -->
				<p:bean class="spring.ioc.Point">
				</p:bean>
			</p:list>
		</p:property>
		<p:property name="numbers">
			<p:map>
				<p:entry>
					<p:key>
						<p:value>One</p:value>
					</p:key>
					<p:value>1</p:value>
				</p:entry>
				<p:entry>
					<p:key>
						<p:value>Two</p:value>
					</p:key>
					<p:value>2</p:value>
				</p:entry>
				<p:entry>
					<p:key>
						<p:value>Three</p:value>
					</p:key>
					<p:value>3</p:value>
				</p:entry>
			</p:map>
		</p:property>
	</p:bean>	
	
</p:beans>
